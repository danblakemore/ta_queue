<% if (window.queue.currentUser().isTa) { %>
    <button id="activate_queue"><%= queue.get('active') ? "Deactivate" : "Activate" %></button>
    <button id="freeze_queue" type="button" ><%= queue.get('frozen') ? "Unfreeze" : "Freeze" %></button>
<% } else { %>
  <% var active = window.queue.get('active');
     var in_queue = window.queue.currentUser().get('in_queue');
     var frozen = window.queue.get('frozen'); %>
  <button id="enter_queue" type="button" <%= ( in_queue || (active && !frozen)) ? "" : 'disabled="disabled"' %> >
    <%= current_user.get('in_queue') ? "Exit Queue" : "Enter Queue" %>
  </button>
<% } %>
<button id="sign_out" type="button" class="sign_out_with">Sign Out</button> 
