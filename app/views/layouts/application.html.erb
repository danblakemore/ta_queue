<!DOCTYPE html>
<html>
<head>
  <title><%= if content_for? :title
               @board.title 
             else
               "Queue"
             end
         %></title>
  <% # stylesheet_link_tag    "application" 
  %>
  <%= yield :stylesheets %>
  <%= javascript_include_tag "application" %>
  <% if params[:iframe] %>
    <%= stylesheet_link_tag "iframe" %>
  <% end %>
  <%= yield :javascripts %>
  <%= csrf_meta_tags %>
  <%= yield :head %>
</head>
<body>

<div id="content">
	<%= yield %>
</div>

</body>
</html>
